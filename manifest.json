{
    "manifest_version": 3,
    "name": "Easy Apply Extension",
    "version": "1.0",
    "description": "Sends a message from popup to content script",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "scripting"],
    "icons": {
      "128": "icon128.png",
      "500": "icon500.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  